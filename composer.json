{
  "name": "carlonicora/raw",
  "description": "Raw API for Discord",
  "version": "3.0.3",
  "homepage": "https://carlonicora.com",
  "time": "2021-04-08",
  "type": "project",
  "minimum-stability": "beta",
  "authors": [
    {
      "name": "Carlo Nicora",
      "email": "carlo@carlonicora.com",
      "homepage": "http://carlonicora.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-json": "*",
    "ext-curl": "*",
    "carlonicora/jsonapi": "~3.0",
    "carlonicora/minimalism": "~13.0",
    "carlonicora/minimalism-service-mysql": "~5.0",
    "carlonicora/minimalism-service-builder": "~2.0.0-beta1",
    "carlonicora/minimalism-service-encrypter": "~3.0",
    "ocramius/package-versions": "~2.1",
    "discord/interactions": "^1.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "CarloNicora\\Minimalism\\Raw\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CarloNicora\\Minimalism\\Raw\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": "bash vendor/bin/minimalism-post-install.sh"
  }
}
